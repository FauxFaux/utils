#!/bin/sh

# $Id: cunw,v 1.7 2000/11/28 11:29:34 sunny Exp $
# Lister ut alle filer som ikke er registrert i CVS (Cvs-UNWanted)

# BUGS: Avhengig av utskriftsformatet til cvs(1)

(cvs -n stat `find . -type d | grep -v 'CVS$'` >/dev/null) 2>&1 | grep -v -e '^cvs status: Examining ' -e '^cvs server: Examining '
(cvs -n stat `find . -type f | grep -v -e 'CVS/Entries' -e 'CVS/Repository$' -e 'CVS/Root$' -e 'CVS/Tag$' ` >/dev/null) 2>&1
