#!/usr/bin/perl -w

#=======================================================================
# $Id$
# Stripper bort oversettelser i .po-filer for å se om det er noe grums 
# som har oppstått der.
#=======================================================================

use strict;

my $Print = 1;

while (<>) {
    if (/^msgid/ || /^$/) {
        $Print = 1;
    }
    elsif (/^msgstr/) {
        $Print = 0;
    }
    $Print && print;
}

# vim: set ts=4 sw=4 sts=4 et :
