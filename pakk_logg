ps auxwOT | grep -v "grep logging" | grep logging &&
    { echo $0: Logging in action, aborting >&2; exit 1; }
cd ~/log/script || { echo $0: ~/log/script: Unable to chdir; exit 1; }
gzip -v *
ciall Created with pakk_logg
git pa
