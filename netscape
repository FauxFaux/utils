#!/bin/sh

# Latskapen lenge leve.
# $Id: netscape,v 1.3 1999/11/15 02:06:07 sunny Exp $

HNAME=`hostname -f`

cd
if [ "$HNAME" = "war.jgaa.com" ]; then
	/home/usr/X11R6/bin/netscape &
elif [ "$HNAME" = "omask.sunbase.org" ]; then
	echo "$0: Netscape ligger ikke her." >&2
else
	netscape-navigator &>/dev/null &
fi
cd -
