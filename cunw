#!/bin/sh

# $Id: cunw,v 1.2 1999/04/24 18:08:41 sunny Exp $
# Lister ut alle filer som ikke er registrert i CVS (Cvs-UNWanted)

# BUGS: Avhengig av utskriftsformatet til cvs(1)

(cvs stat `find . -type f | grep -v -e 'CVS/Entries$' -e 'CVS/Repository$' -e 'CVS/Root$' ` >/dev/null) 2>&1 | cut -c 50-
