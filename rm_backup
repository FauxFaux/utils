#!/bin/sh

# $Id: rm_backup,v 1.2 1999/04/23 12:55:48 sunny Exp $

find -name '*~' -type f -exec rm -v {} \;
exit

## Gammel måte har vi her:

_fjernes=`find . -type f | grep '~$'`

if [ ! "$_fjernes" = "" ]; then
  rm -v $_fjernes
else
  echo `basename $0`: Ingen backupfiler ble funnet >&2
fi


#### End of file $Id: rm_backup,v 1.2 1999/04/23 12:55:48 sunny Exp $ ####
