#!/bin/bash

#=======================================================================
# $URL$
# $Id$
# Setter properties på tekstfiler sånn som jeg vil ha det.
# Hvis -u spesifiseres som første parameter, settes ikke svn:eol-style.
#=======================================================================

if [ "$1" != "-u" ]; then
    svn propset svn:eol-style native $*
else
    shift
fi
svn propset svn:keywords "Id URL" $*
svn propdel cvs2svn:cvs-rev $*
svn proplist -v $*

# vim: set ts=4 sw=4 sts=4 et fo+=w fenc=utf8 :
