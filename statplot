#!/usr/bin/gnuplot -persist

# $Id: statplot,v 1.4 2003/10/02 00:52:21 sunny Exp $
# Lager fin graf over ting som ligger i filen "data".

# set xdata time
# set timefmt "%m/%d/%y"
# set xrange ["03/21/95":"03/22/95"]
# set format x "%m/%d"
# set timefmt "%m/%d/%y %H:%M"
# plot "data" using 1:3

set xdata time
set grid
set timefmt "%Y-%m-%dT%H:%M:%SZ"
set format x "%Y-%m-%d"
# set terminal postscript
# set output "output.ps"
set term x11
set title "`cat data.title`"
plot \
"data" using 1:2 w l 7, \
"data" using 1:2 s bezier w lp -1
# "data" using 1:2 s bezier w l -1, \
# "data" using 1:2 s sbezier w l 29

